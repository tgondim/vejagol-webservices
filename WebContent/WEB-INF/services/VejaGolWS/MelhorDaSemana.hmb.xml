<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="com.vejagol.android.model.MelhorDaSemana" table="melhor_da_semana" lazy="true">
		<id name="id" column="ID" unsaved-value="0">
			<generator class="increment">
				<param name="sequence">GEN_MELHOR_DA_SEMANA_ID</param>
			</generator>
		</id>
		<property name="mes" column="MES" update="true" />
		<property name="semana" column="SEMANA" update="true"/>
		<property name="ano" column="ANO" update="true"/>
		<property name="link" column="LINK" update="true"/>
	</class>
	
</hibernate-mapping>  
